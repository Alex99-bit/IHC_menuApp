<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".prueba">

    <ImageButton
        android:id="@+id/benito2"
        android:layout_width="280dp"
        android:layout_height="108dp"
        android:contentDescription="Billete De 500 pesos"
        app:layout_constraintBottom_toTopOf="@+id/guideline202"
        app:layout_constraintEnd_toStartOf="@+id/guideline103"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline201"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/benito"
        android:onClick="EscribirQuinientos"
        />

    <TextView
        android:id="@+id/tvResultado"
        android:layout_width="307dp"
        android:layout_height="158dp"
        android:layout_margin="5dp"
        android:background=" #000"
        android:gravity="right|center_vertical"
        android:text="0"
        android:textColor="@color/white"
        android:textSize="100dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline201"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline102"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageButton
        android:id="@+id/sorjuana"
        android:layout_width="280dp"
