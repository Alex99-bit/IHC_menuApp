        android:onClick="EscribirDos"
        />

    <ImageButton
        android:id="@+id/uno"
        android:layout_width="94dp"
        android:layout_height="108dp"
        android:contentDescription="moneda 1 peso"
        app:layout_constraintBottom_toTopOf="@+id/guideline205"
        app:layout_constraintEnd_toStartOf="@+id/guideline105"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/guideline103"
        app:layout_constraintTop_toTopOf="@+id/guideline204"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/uno"
        android:onClick="EscribirUno"
        />


    <ImageButton
        android:id="@+id/borrar"
        android:layout_width="88dp"
        android:layout_height="119dp"
        android:contentDescription="borrar"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
