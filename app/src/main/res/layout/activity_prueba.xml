        app:srcCompat="@drawable/sorjuana"
        android:onClick="EscribirDoscientos"
        />


    <ImageButton
        android:id="@+id/neza"
        android:layout_width="280dp"
        android:layout_height="108dp"
        android:contentDescription="Billete De 100 pesos"
        app:layout_constraintBottom_toTopOf="@+id/guideline204"
        app:layout_constraintEnd_toStartOf="@+id/guideline103"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline203"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/neza"
        android:onClick="EscribirCien"
        />

    <ImageButton
        android:id="@+id/morelos"
        android:layout_width="280dp"
        android:layout_height="108dp"
        android:contentDescription="Billete De 50 pesos"
        app:layout_constraintBottom_toTopOf="@+id/guideline205"
        app:layout_constraintEnd_toStartOf="@+id/guideline103"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline204"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/morelos"
        android:onClick="EscribirCincuenta"
        />

    <ImageButton
        android:id="@+id/beni3"
        android:layout_width="290dp"
        android:layout_height="129dp"
        android:contentDescription="Billete De 20 pesos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline103"
        app:layout_constraintHorizontal_bias="1.0"
